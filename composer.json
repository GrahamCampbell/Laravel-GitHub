{
    "name": "graham-campbell/github",
    "description": "GitHub Is A GitHub Bridge For Laravel",
    "keywords": ["laravel", "framework", "github", "php-github-api", "PHP GitHub API", "github bridge", "bridge", "GitHub", "Laravel GitHub", "Laravel-GitHub", "Graham Campbell", "GrahamCampbell"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "^5.5|^6.0|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0",
        "graham-campbell/manager": "^4.4",
        "knplabs/github-api": "2.13.*",
        "php-http/client-common": "^1.9|^2.0",
        "php-http/cache-plugin": "^1.6",
        "symfony/cache": "^3.3|^4.0|^5.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.4",
        "graham-campbell/testbench": "^5.4",
        "lcobucci/jwt": "^3.3",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^6.5|^7.0|^8.0",
        "php-http/guzzle6-adapter": "^1.1|^2.0"
    },
    "suggest": {
        "lcobucci/jwt": "Allows using the private key authenticator (^3.3)"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\GitHub\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\GitHub\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.7-dev"
        },
        "laravel": {
            "providers": [
                "GrahamCampbell\\GitHub\\GitHubServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
